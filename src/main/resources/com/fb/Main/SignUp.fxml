<AnchorPane prefHeight="484.0" prefWidth="559.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.fb.Main.Controller">
   <children>
      <TextField fx:id="EmailTextField" layoutX="208.0" layoutY="178.0" />
      <DatePicker fx:id="myDatePicker" layoutX="195.0" layoutY="355.0" />
      <TextField fx:id="NameTextField" layoutX="208.0" layoutY="131.0" />
      <PasswordField fx:id="PasswordField" layoutX="208.0" layoutY="229.0" />
      <PasswordField fx:id="RepasswordField" layoutX="208.0" layoutY="277.0" />
      <RadioButton fx:id="MaleRadioButton" layoutX="208.0" layoutY="320.0" mnemonicParsing="false" onAction="#getGender" text="Male">
         <toggleGroup>
            <ToggleGroup fx:id="Gender" />
         </toggleGroup></RadioButton>
      <RadioButton fx:id="FemaleRadioButton" layoutX="298.0" layoutY="320.0" mnemonicParsing="false" onAction="#getGender" text="Female" toggleGroup="$Gender" />
      <Button fx:id="SignUpButton" layoutX="254.0" layoutY="400.0" mnemonicParsing="false" onAction="#switchToLogin" text="Sign up" />
      <Label layoutX="214.0" layoutY="25.0" prefHeight="90.0" prefWidth="138.0" text="Buzz">
         <font>
            <Font size="61.0" />
         </font>
      </Label>
      <Button contentDisplay="CENTER" graphicTextGap="2.0" layoutX="328.0" layoutY="445.0" mnemonicParsing="false" onAction="#switchToLogin" prefHeight="25.0" prefWidth="42.0" text="Login">
         <font>
            <Font size="10.0" />
         </font>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </Button>
      <Label layoutX="186.0" layoutY="445.0" prefHeight="25.0" prefWidth="139.0" text="Already have an account ?" />
   </children>
</AnchorPane>
